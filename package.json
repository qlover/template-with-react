{
  "name": "test",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@yueqing/lint": "^2.3.1",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.2.6",
    "craco": "0.0.3",
    "craco-less": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.2",
    "postcss": "^8.4.20",
    "release-it": "^15.6.0",
    "tailwindcss": "^3.2.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "lint": "eslint --ext .js,.ts,.tsx --format=pretty ./src",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx --format=pretty ./src",
    "prepare": "husky install",
    "eject": "react-scripts eject",
    "git:commit:add": "git add . & cz",
    "git:commit": "cz",
    "init:cz": "npx commitizen init cz-conventional-changelog --save-dev --save-exact",
    "init:release": "npm init release-it",
    "release": "release-it"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
